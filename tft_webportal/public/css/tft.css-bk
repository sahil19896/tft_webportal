@import url("https://fonts.googleapis.com/css?family=Francois+One&display=swap");

:root {
    --ink: #1d1f1d;
    --muted: #6d736f;
    --paper: #f6f7f6;
    --accent: #185a2a;
    --accent-2: #2d3a33;
    --line: #e1e5e2;
    --surface: #ffffff;
    --surface-2: #f1f3f1;
    --shadow: 0 12px 28px rgba(18, 23, 20, 0.08);
}

html,
body,
.page-container,
.page-content {
    background: #ffffff !important;
}

.navbar-light {
    border-bottom: 1px solid #e2e2e2;
    background: #69b07926 !important;
}

.tft-register {
    background: #ffffff;
    min-height: 100vh;
    padding: 56px 0 80px;
    font-family: "Francois One", arial, serif;
    color: var(--ink);
    position: relative;
    overflow: hidden;
}

.tft-register #particles-js {
    position: fixed;
    inset: 0;
    z-index: 0;
    pointer-events: none;
}

.tft-register .wrap {
    position: relative;
    z-index: 1;
    max-width: 1100px;
    margin: 0 auto;
    padding: 0 24px;
}

.tft-register .header {
    display: grid;
    gap: 12px;
    margin-bottom: 32px;
}

.tft-register .eyebrow {
    text-transform: uppercase;
    font-size: 12px;
    letter-spacing: 0.2em;
    color: var(--muted);
}

.tft-register h1 {
    font-size: clamp(32px, 4vw, 46px);
    margin: 0;
}

.tft-register .subhead {
    max-width: 560px;
    color: var(--muted);
    margin: 0;
}

.tft-register .grid {
    display: grid;
    grid-template-columns: minmax(280px, 2fr);
    gap: 28px;
    align-items: start;
}

.tft-register .card {
    background: var(--surface);
    border: 1px solid var(--line);
    border-radius: 22px;
    padding: 28px;
    box-shadow: var(--shadow);
    width: 100%;
}

.tft-register .stack {
    display: grid;
    gap: 18px;
}

.tft-register .is-hidden {
    display: none;
}

.tft-register .field {
    display: grid;
    gap: 8px;
}

.tft-register .field-message {
    font-size: 12px;
    color: #991b1b;
    min-height: 16px;
}

.tft-register .field-message:empty {
    display: none;
}

.tft-register .field.has-error input,
.tft-register .field.has-error select {
    border-color: #991b1b;
    box-shadow: 0 0 0 3px rgba(153, 27, 27, 0.12);
    background: rgba(153, 27, 27, 0.04);
}

.tft-register .field.has-error label {
    color: #991b1b;
}

.tft-register label {
    font-size: 13px;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    color: var(--muted);
}

.tft-register input,
.tft-register select {
    border: 1px solid var(--line);
    background: var(--surface);
    border-radius: 14px;
    padding: 12px 14px;
    font-size: 15px;
    transition:
        border 0.2s ease,
        box-shadow 0.2s ease;
}

.tft-register input:focus,
.tft-register select:focus {
    outline: none;
    border-color: var(--accent);
    box-shadow: 0 0 0 3px rgba(24, 90, 42, 0.18);
}

.tft-register .split {
    display: grid;
    grid-template-columns: repeat(2, minmax(0, 1fr));
    gap: 14px;
}

.tft-register .consent {
    display: flex;
    gap: 10px;
    align-items: flex-start;
    font-size: 14px;
    color: var(--muted);
}

.tft-register .consent input {
    margin-top: 4px;
}

.tft-register .actions {
    display: flex;
    flex-wrap: wrap;
    gap: 12px;
    align-items: center;
}

.tft-register .primary {
    border: none;
    background: linear-gradient(120deg, var(--accent), #2b7a42);
    color: #f5f7f5;
    padding: 12px 22px;
    /* border-radius: 999px; */
    font-weight: 600;
    cursor: pointer;
    transition:
        transform 0.2s ease,
        box-shadow 0.2s ease;
}

.tft-register .primary:hover {
    transform: translateY(-1px);
    box-shadow: 0 8px 20px rgba(24, 90, 42, 0.28);
}

.btn.btn-primary {
    background: linear-gradient(120deg, var(--accent), #2b7a42);
    border-color: var(--accent);
    color: #f5f7f5;
}

.btn.btn-primary:hover,
.btn.btn-primary:focus {
    background: linear-gradient(120deg, #1c6a32, #2f8547);
    border-color: #1c6a32;
    color: #f5f7f5;
}

.tft-register .primary.is-loading {
    opacity: 0.7;
    cursor: wait;
}

.tft-register .secondary {
    border: 1px solid var(--line);
    background: var(--surface);
    padding: 10px 18px;
    /* border-radius: 999px; */
    color: var(--accent-2);
    text-decoration: none;
    font-size: 14px;
}

.tft-register .aside {
    display: grid;
    gap: 16px;
    border-left: 2px solid rgba(211, 154, 51, 0.2);
    padding-left: 20px;
}

.tft-register .aside h2 {
    font-size: 24px;
    margin: 0;
}

.tft-register .aside p {
    margin: 0;
    color: var(--muted);
}

.tft-register .badge {
    display: inline-flex;
    gap: 8px;
    align-items: center;
    background: rgba(211, 154, 51, 0.12);
    color: #8c6a1f;
    padding: 8px 12px;
    /* border-radius: 999px; */
    font-size: 13px;
    font-weight: 600;
}

.tft-register .stats {
    display: grid;
    grid-template-columns: repeat(2, minmax(0, 1fr));
    gap: 12px;
}

.tft-register .stat {
    background: var(--surface);
    border-radius: 14px;
    padding: 14px;
    border: 1px solid var(--line);
}

.tft-register .stat strong {
    display: block;
    font-size: 20px;
    color: #8c6a1f;
}

.tft-register .footer-note {
    margin-top: 12px;
    font-size: 12px;
    color: var(--muted);
}

.tft-register .verification-status {
    padding-top: 10px;
}

.tft-register .status-card {
    border-radius: 16px;
    border: 1px solid var(--line);
    padding: 18px;
    background: var(--surface);
    box-shadow: var(--shadow);
}

.tft-register .status-card h2 {
    margin: 0 0 8px;
    font-size: 20px;
}

.tft-register .status-card p {
    margin: 0;
    color: var(--muted);
}

.tft-register .status-success {
    border-color: rgba(24, 90, 42, 0.3);
    background: rgba(24, 90, 42, 0.06);
}

.tft-register .status-error {
    border-color: rgba(153, 27, 27, 0.3);
    background: rgba(153, 27, 27, 0.06);
}

@media (max-width: 900px) {
    .tft-register .grid {
        grid-template-columns: 1fr;
    }

    .tft-register .aside {
        border-left: none;
        padding-left: 0;
        border-top: 1px solid var(--line);
        padding-top: 20px;
    }
}
